PROC B_RING_GROOVE DISPLOF
;;**********程序功能**********
;;环形槽
;;***************************

DEF REAL SPEED=1500

IF $A_DBB[6]==1;C轴旋转键选中
    F_GRINDWHEEL_RUN(SPEED);砂轮启动

    C_CYCLE:
    MSG("头架旋转中...")
    IF GRIND[0]<>1;不是内螺纹
        G91 C=360000 F=INI[58];头架旋转
    ELSE
        G91 C=-360000 F=INI[58];头架旋转
    ENDIF
    GOTOB C_CYCLE
ENDIF

RET


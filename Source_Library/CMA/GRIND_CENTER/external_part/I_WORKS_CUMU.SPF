PROC I_WORKS_CUMU DISPLOF
;***********程序功能**********
;*工件计数
;****************************

IF INI[97]<>0;0是单件磨削模式,1是批量磨削模式
    INI[93]=INI[93]+1;磨削工件计数
ENDIF

IF (INI[94]<>0) AND (INI[93]>=INI[94]);磨削几件后修整不为零或当前几件大于设定值
    IF INI[93]/INI[94]-TRUNC(INI[93]/INI[94])==0
        INI[95]=1;标记位
    ENDIF
ENDIF

RET

